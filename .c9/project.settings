{
    "ace": {
        "@guessTabSize": "true",
        "@newLineMode": "unix",
        "@tabSize": "4",
        "@useSoftTabs": "true"
    },
    "build": {
        "@builder": "auto",
        "@path": "/.c9/builders",
        "@saveall": true
    },
    "find.nak": {
        "@searchLimit": 100
    },
    "language": {
        "@instanceHighlight": "true",
        "@semi": "true",
        "@undeclaredVars": "true",
        "@unusedFunctionArgs": true,
        "@warnLevel": "info"
    },
    "run": {
        "@path": "/.c9/runners",
        "@runner": "Apache httpd (PHP, HTML)",
        "configs": {
            "@inited": "true",
            "json()": {
                "Web Server": {
                    "command": "",
                    "debug": true,
                    "name": "Web Server",
                    "runner": "Apache httpd (PHP, HTML)"
                }
            }
        }
    },
    "share": {
        "@app": false,
        "@preview": false,
        "@useOwnerSettings": true
    }
}